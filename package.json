    "@mantine/notifications": "^7.15.3",
    "@tabler/icons-react": "^3.27.1",
    "@tanstack/react-query": "^5.62.16",
    "@tanstack/react-query-devtools": "^5.62.16",
    "jotai": "^2.11.0",
    "jotai-tanstack-query": "^0.9.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.1"
